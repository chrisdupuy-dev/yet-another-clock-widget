    </entry>
    <entry name="dateFontStyleName" type="String">
      <default>Regular</default>
    </entry>
    <entry name="dateFontStrikeout" type="Bool">
      <default>false</default>
    </entry>
    <entry name="dateFontUnderline" type="Bool">
      <default>false</default>
    </entry>
    <entry name="dateStrokeEnabled" type="Bool">
      <default>false</default>
    </entry>
    <entry name="dateStrokeColorButton" type="Color">
      <default>#000000</default>
    </entry>
    <entry name="dateStrokeColorText" type="String">
      <default>#000000</default>
    </entry>
    <entry name="dateStrokeSize" type="Double">
      <default>1</default>
    </entry>
    <entry name="dateDropShadowEnabled" type="Bool">
      <default>false</default>
    </entry>
    <entry name="dateDropShadowOpacity" type="Double">
      <default>1.0</default>
    </entry>
    <entry name="dateDropShadowBlur" type="Double">
      <default>0.6</default>
    </entry>
    <entry name="dateDropShadowScale" type="Double">
      <default>1.0</default>
    </entry>
    <entry name="dateDropShadowHorizontalOffset" type="Double">
      <default>5.0</default>
    </entry>
    <entry name="dateDropShadowVerticalOffset" type="Double">
      <default>5.0</default>
    </entry>
    <entry name="dateDropShadowColorButton" type="Color">
      <default>#000000</default>
    </entry>
    <entry name="dateDropShadowColorText" type="String">
      <default>#000000</default>
    </entry>
